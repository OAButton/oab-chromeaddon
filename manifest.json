{
  "name": "Open Access Button",
  "version": "0.3",
  "manifest_version": 3,
  "description": "Push Button. Get Research. Make Progress.",
  "icons": { "128": "img/oa70.png" },
  "browser_action": {
    "default_title": "OA Button",
    "default_icon": "img/oa64.png",
    "default_popup": "ui/login.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
